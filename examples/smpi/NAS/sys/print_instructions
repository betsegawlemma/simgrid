echo ''
echo '   To make a NAS benchmark type '
echo ''
echo '         make <benchmark-name> NPROCS=<number> CLASS=<class>'
echo ''
echo '   where <benchmark-name>  is "ep", "dt", or "is"
echo '         <number>          is the number of processors'
echo '         <class>           is "S", "W", "A", "B", "C", or "D"'
echo ''

